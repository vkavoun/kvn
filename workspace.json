{
  "version": 1,
  "projects": {
    "kvn-game": {
      "root": "apps/kvn-game",
      "sourceRoot": "apps/kvn-game",
      "projectType": "application",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@nrwl/next:build",
          "options": {
            "root": "apps/kvn-game",
            "outputPath": "dist/apps/kvn-game"
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "environments/environment.ts",
                  "with": "environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@nrwl/next:dev-server",
          "options": {
            "buildTarget": "kvn-game:build",
            "dev": true
          },
          "configurations": {
            "production": {
              "buildTarget": "kvn-game:build:production",
              "dev": false
            }
          }
        },
        "export": {
          "builder": "@nrwl/next:export",
          "options": {
            "buildTarget": "kvn-game:build:production"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:lint",
          "options": {
            "linter": "eslint",
            "config": "apps/kvn-game/.eslintrc",
            "tsConfig": [
              "apps/kvn-game/tsconfig.json",
              "apps/kvn-game/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!apps/kvn-game/**"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/kvn-game/jest.config.js",
            "tsConfig": "apps/kvn-game/tsconfig.spec.json",
            "passWithNoTests": true
          }
        }
      }
    },
    "kvn-game-e2e": {
      "root": "apps/kvn-game-e2e",
      "sourceRoot": "apps/kvn-game-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/kvn-game-e2e/cypress.json",
            "tsConfig": "apps/kvn-game-e2e/tsconfig.e2e.json",
            "devServerTarget": "kvn-game:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "kvn-game:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:lint",
          "options": {
            "linter": "eslint",
            "config": "apps/kvn-game-e2e/.eslintrc",
            "tsConfig": ["apps/kvn-game-e2e/tsconfig.e2e.json"],
            "exclude": ["**/node_modules/**", "!apps/kvn-game-e2e/**"]
          }
        }
      }
    },
    "ui": {
      "root": "libs/ui",
      "sourceRoot": "libs/ui/src",
      "projectType": "library",
      "schematics": {},
      "architect": {
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "libs/ui/tsconfig.lib.json",
              "libs/ui/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!libs/ui/**"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/ui/jest.config.js",
            "tsConfig": "libs/ui/tsconfig.spec.json",
            "passWithNoTests": true
          }
        }
      }
    },
    "sandbox": {
      "root": "apps/sandbox",
      "sourceRoot": "apps/sandbox",
      "projectType": "application",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@nrwl/next:build",
          "options": {
            "root": "apps/sandbox",
            "outputPath": "dist/apps/sandbox"
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/sandbox/environments/environment.ts",
                  "with": "apps/sandbox/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "build-dev": {
          "builder": "@nrwl/node:build",
          "options": {
            "outputPath": "dist/apps/sandbox",
            "main": "apps/sandbox/main",
            "tsConfig": "apps/sandbox/tsconfig.json",
            "assets": ["apps/sandbox/assets"]
          }
        },
        "serve-custom": {
          "builder": "@nrwl/node:execute",
          "options": {
            "buildTarget": "sandbox:build-dev"
          }
        },
        "serve": {
          "builder": "@nrwl/next:dev-server",
          "options": {
            "buildTarget": "sandbox:build",
            "dev": true
          },
          "configurations": {
            "production": {
              "buildTarget": "sandbox:build:production",
              "dev": false
            }
          }
        },
        "export": {
          "builder": "@nrwl/next:export",
          "options": {
            "buildTarget": "sandbox:build:production"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:lint",
          "options": {
            "linter": "eslint",
            "config": "apps/sandbox/.eslintrc",
            "tsConfig": [
              "apps/sandbox/tsconfig.json",
              "apps/sandbox/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!apps/sandbox/**"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/sandbox/jest.config.js",
            "tsConfig": "apps/sandbox/tsconfig.spec.json",
            "passWithNoTests": true
          }
        }
      }
    },
    "sandbox-e2e": {
      "root": "apps/sandbox-e2e",
      "sourceRoot": "apps/sandbox-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/sandbox-e2e/cypress.json",
            "tsConfig": "apps/sandbox-e2e/tsconfig.e2e.json",
            "devServerTarget": "sandbox:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "sandbox:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:lint",
          "options": {
            "linter": "eslint",
            "config": "apps/sandbox-e2e/.eslintrc",
            "tsConfig": ["apps/sandbox-e2e/tsconfig.e2e.json"],
            "exclude": ["**/node_modules/**", "!apps/sandbox-e2e/**"]
          }
        }
      }
    },
    "graphics": {
      "root": "libs/graphics",
      "sourceRoot": "libs/graphics/src",
      "projectType": "library",
      "schematics": {},
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:lint",
          "options": {
            "linter": "eslint",
            "config": "libs/graphics/.eslintrc",
            "tsConfig": [
              "libs/graphics/tsconfig.lib.json",
              "libs/graphics/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!libs/graphics/**"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/graphics/jest.config.js",
            "tsConfig": "libs/graphics/tsconfig.spec.json",
            "passWithNoTests": true
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/next"
  },
  "schematics": {
    "@nrwl/workspace": {
      "library": {
        "linter": "eslint"
      }
    },
    "@nrwl/cypress": {
      "cypress-project": {
        "linter": "eslint"
      }
    },
    "@nrwl/react": {
      "application": {
        "style": "scss",
        "linter": "eslint",
        "babel": true
      },
      "component": {
        "style": "scss"
      },
      "library": {
        "style": "scss",
        "linter": "eslint"
      }
    },
    "@nrwl/next": {
      "application": {
        "style": "scss",
        "linter": "eslint"
      }
    },
    "@nrwl/web": {
      "application": {
        "linter": "eslint"
      }
    },
    "@nrwl/node": {
      "application": {
        "linter": "eslint"
      },
      "library": {
        "linter": "eslint"
      }
    },
    "@nrwl/nx-plugin": {
      "plugin": {
        "linter": "eslint"
      }
    },
    "@nrwl/nest": {
      "application": {
        "linter": "eslint"
      }
    },
    "@nrwl/express": {
      "application": {
        "linter": "eslint"
      }
    }
  },
  "defaultProject": "kvn-game"
}
